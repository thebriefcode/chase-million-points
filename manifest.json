{
  "manifest_version": 3,
  "name": "Chase Those Million Points",
  "version": "1.0",
  "description": "",
  "permissions": ["tabs", "activeTab", "scripting", "storage", "alarms"],
  "background": {
    "service_worker": "dist/service-worker.js"
  },
  "host_permissions": ["https://millionpoints.chase.com/"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon128.png"
    }
  },
  "icons": {
    "128": "icon128.png"
  }
}
